    "aws-cdk": "2.66.1",
    "changelog-verify": "^1.1.2",
    "esbuild": "^0.17.11",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5",
    "version-changelog": "^3.1.1"
