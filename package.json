    "cognito-backup": "bin/cognito-backup.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:boillodmanuel/aws-cognito-backup-restore.git"
  },
  "scripts": {
    "getVersion": "node -e \"console.log(require('./package.json').version)\"",
